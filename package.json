{
  "name": "njxcontrol-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "pnpm lint && pnpm typecheck && bash scripts/conditional-migrate.sh",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "db:info": "bash scripts/db-info.sh",
    "db:status": "bash scripts/db-status.sh",
    "db:check": "bash scripts/check-migrations.sh",
    "db:migrate": "bash scripts/migrate-production.sh",
    "db:seed": "node --env-file=.env.local --import tsx scripts/db-seed.ts",
    "db:setup": "bash scripts/setup-local-db.sh",
    "db:reset": "node scripts/reset-local-db.js",
    "db:reset:sh": "bash scripts/reset-local-db.sh",
    "db:reset:ps1": "powershell -ExecutionPolicy Bypass -File scripts/reset-local-db.ps1",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "branch:validate": "bash scripts/validate-branch.sh",
    "branch:validate:main": "bash scripts/validate-branch.sh main",
    "assets:icons": "node --import tsx scripts/generate-icons.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^0.5.5",
    "@react-email/render": "^1.3.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tanstack/react-table": "^8.21.3",
    "@vis.gl/react-google-maps": "^1.5.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "geist": "^1.5.1",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "next-view-transitions": "^0.3.4",
    "nextjs-toploader": "^3.8.16",
    "react": "^19.1.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-number-format": "^5.4.4",
    "react-phone-number-input": "^3.2.19",
    "react-time-picker": "^8.0.2",
    "resend": "^6.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "use-debounce": "^10.0.5",
    "vercel": "^48.2.9",
    "zod": "^4.0.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@next/eslint-plugin-next": "^15.4.6",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "shadcn": "^3.4.2",
    "supabase": "^2.54.11",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4",
    "sharp": "^0.33.5",
    "png-to-ico": "^2.1.8"
  },
  "packageManager": "pnpm@10.20.0",
  "browser": {
    "ws": false,
    "encoding": false,
    "crypto": false,
    "stream": false,
    "util": false,
    "buffer": false
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "*.{json,md,css,scss}": [
      "prettier -w"
    ]
  }
}
